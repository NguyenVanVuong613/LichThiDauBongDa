<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.lichthidaubongda.fragment.home.view.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_scheduleFragment"
            app:destination="@id/scheduleFragment" />
        <action
            android:id="@+id/action_homeFragment_to_chartFragment"
            app:destination="@id/chartFragment" />
        <action
            android:id="@+id/topscorerFragment2"
            app:destination="@id/topscorerFragment" />
        <action
            android:id="@+id/topassist"
            app:destination="@id/topassistFragment" />
    </fragment>
    <fragment
        android:id="@+id/scheduleFragment"
        android:name="com.example.lichthidaubongda.fragment.schedule.view.ScheduleFragment"
        android:label="@string/Schedule"
        tools:layout="@layout/fragment_schedule" />
    <fragment
        android:id="@+id/chartFragment"
        android:name="com.example.lichthidaubongda.fragment.chart.view.ChartFragment"
        android:label="@string/Chart"
        tools:layout="@layout/fragment_chart" />
    <fragment
        android:id="@+id/topscorerFragment"
        android:name="com.example.lichthidaubongda.fragment.topscorer.view.TopscorerFragment"
        android:label="@string/topscorer"
        tools:layout="@layout/fragment_topscorer"/>
    <fragment
        android:id="@+id/topassistFragment"
        android:name="com.example.lichthidaubongda.fragment.topassist.view.TopassistFragment"
        android:label="@string/topassist"
        tools:layout="@layout/fragment_topassist" />
</navigation>